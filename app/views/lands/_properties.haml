- @properties.each do |p|
  = render partial: 'lands/property_thumb', locals: {property: p}
.clearfix
- if @properties.next_page
  = link_to 'show more', interests_lands_path(params.permit!.merge({page: params[:page].present? ? params[:page].to_i + 1 : 2})), remote: true, class: 'more-interests-link'