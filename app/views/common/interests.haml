= render partial: 'layouts/sub_nav', locals: {remote: true}
- @properties_users.each do |interest|
  = render partial: 'common/property_thumb', locals: {property: interest.property, user: interest.user}
- if @properties_users.next_page
  = link_to 'show more', eval("interests_#{@klass_underscore.pluralize}_path(page: params[:page].present? ? params[:page].to_i + 1 : 2)"), remote: true, class: 'more-interests-link'
