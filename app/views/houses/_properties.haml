- @properties.each do |p|
  = render partial: 'houses/property_thumb', locals: {property: p}
- if @properties.count == 0
  .no-results-message.text-center
    No Matching Houses Found
.clearfix
- if @properties.next_page
  = link_to 'show more', houses_path(params.permit!.merge({page: params[:page].present? ? params[:page].to_i + 1 : 2})), remote: true, class: 'more-interests-link'